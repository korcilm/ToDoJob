    <link href="~/theme/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <!-- Font awesome -->
    <link href="~/lib/fontawesome/css/all.min.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="~/lib/template-css/simple-sidebar.css" rel="stylesheet" />

    <!-- Theme css files -->
    <link href="~/theme/css/style.css" rel="stylesheet" />
    <link href="~/theme/css/style-responsive.css" rel="stylesheet" />

    <link href="~/theme/css/zabuto_calendar.css" rel="stylesheet" />
