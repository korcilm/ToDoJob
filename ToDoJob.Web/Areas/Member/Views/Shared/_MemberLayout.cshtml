    <script src="~/theme/lib/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/theme/lib/jquery.scrollTo.min.js"></script>
    <script src="~/theme/lib/jquery.nicescroll.js" type="text/javascript"></script>
    @RenderSection("JavaScript", false)
    <!--common script for all pages-->
    <script src="~/theme/lib/common-scripts.js"></script>
    <script src="~/theme/lib/gritter/js/jquery.gritter.js"></script>
    <script src="~/theme/lib/gritter-conf.js"></script>



